<h1>配送先編集</h1>
<%= form_with(model: @address, url:address_path, local: true ,method: :patch) do |f| %>

  <table>
    <tr>
      <th><%= f.label :"郵便番号(ハイフンなし)" %></th>
      <td><%= f.text_field :postal_code %></td>
    </tr>
    <tr>
      <th><%= f.label :"住所" %></th>
      <td> <%= f.text_field :address %></td>
    </tr>
      <th><%= f.label :"宛名" %></th>
      <td> <%= f.text_field :name %></td>
      <td><%= f.submit '変更内容を保存' %></td>
    </tr>
  </table>
  <% end %>
    
 
